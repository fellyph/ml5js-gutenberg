{
	"name": "product-classifier",
	"version": "1.1.0",
	"private": true,
	"description": "Product classifier",
	"repository": "git+https://github.com/fellyph/ml5js-gutenberg.git",
	"author": "Fellyph Cintra",
	"keywords": [
		"WordPress",
		"editor",
		"Examples"
	],
	"scripts": {
		"lint-js": "wp-scripts lint-js ./product-classifier",
		"lint-js:fix": "npm run lint-js -- --fix",
		"env:start": "docker-compose up -d",
		"env:stop": "docker-compose stop",
		"build": "npm run build:product-classifier",
		"build:product-classifier": "wp-scripts build product-classifier/src/index.js --output-path=product-classifier/build"
	},
	"devDependencies": {
		"@wordpress/components": "^8.1.0",
		"@wordpress/scripts": "^5.0.0"
	}
}